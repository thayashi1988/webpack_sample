{
  "compilerOptions": {
    // "outDir": "./dist/",
    // "jsx": "react",
    "noImplicitAny": false,
    "module": "ES6",
    "target": "ES5",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "removeComments": true,
    "noImplicitReturns": true,
    "types": []
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}